'use babel'
// @flow
import type {DevToolInfos} from "../../TargetConfigurationFeature/Types/types.js.flow";

export type TaskState = "created" | "running" | "finished" | "crashed";

export type Task = {
  id: string,
  target: {
    config: mixed,
    tool: DevToolInfos
  },
  state: TaskState,
};

export type Process = mixed;

export type TaskExecution = {
  task: Task,
  process: Process,
};

export type TaskManager = {
  executions: Array<TaskExecution>
};

export type {DevToolInfos};
